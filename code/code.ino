        delay(5000);
        display.clearDisplay();
        display.drawBitmap(0, 0, QRarray, 128, 64, 1);
        display.display();
        delay(15000);
        display.clearDisplay();  // Clear the buffer
        contador = 0;
      }
      
    motor.step(380); // cantidad de pasos
    delay(1500);      // 2 seg.
    motor.step(-380); // cantidad de pasos
    
    } 

